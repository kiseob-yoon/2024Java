package sec02.exam01;

public class LottoEx {

	public static void main(String[] args) {
//		System.out.println(Math.PI); // 대문자로 시작하는거 시스템에서 가져다쓰라고 준것
		
		for(int i = 0; i < 10; i++) {
			int num = (int)(Math.random()*45); // 0과 같거나 큰수와 45을 곱하고 
			System.out.println(num); //0보다는 같거나 크고 1보다는 작은수가 나온다
		}

	}

}
