package sec03.exam01;

public class keyCodeExample {

	public static void main(String[] args) throws Exception{
		int keyCode;
		
		for(;;) {
		
		System.out.print("입력>");
		keyCode = System.in.read();
		System.out.println(keyCode);
		}
		if(keycode ==113) {
			break;
		}
		


	}
	System.out.println("종료")

}
