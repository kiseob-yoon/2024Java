package sec02.exam01;

public class TypeProblem {

	public static void main(String[] args) {
		byte byteValue =10;
		float floatValue = 2.5F;
		double doubleValue = 2.5;
		
//		float result = 5 + floatValue;
//		System.out.println(result);
		
		double result = 5 + doubleValue;
		System.out.println(result);

	}

}
